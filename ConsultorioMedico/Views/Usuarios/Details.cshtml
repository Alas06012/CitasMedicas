@model ConsultorioMedico.Models.Usuario

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Detalles Usuario</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Usuario:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NomUser)
        </dd>
        <dt class = "col-sm-2">
            Password:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.PassUser)
        </dd>
         <dt class = "col-sm-2">
            Pregunta:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdPreguntaNavigation.IdPreguntas)
        </dd>
        <dt class = "col-sm-2">
            Respuesta:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.RespUser)
        </dd>
       @* <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Estado)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Estado)
        </dd>*@
       
        <dt class = "col-sm-2">
           Rol:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdRolNavigation.IdRol)
        </dd>
    </dl>
</div>
<div>

    <a asp-action="Index" class="btn btn-outline-dark">Volver</a>


</div>
